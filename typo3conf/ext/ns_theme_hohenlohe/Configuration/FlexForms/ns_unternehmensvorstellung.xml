<T3DataStructure>
    <sheets>
        <sDEF>
            <ROOT>
                <TCEforms>
                    <sheetTitle>Fields</sheetTitle>
                </TCEforms>
                <type>array</type>
                <el>
                    <overline>
                        <TCEforms>
                            <label>LLL:EXT:ns_theme_hohenlohe/Resources/Private/Language/locallang_flex.xlf:general.overline</label>
                            <config>
                                <type>input</type>
                                <size>50</size>
                                <eval>trim</eval>
                            </config>
                        </TCEforms>
                    </overline>
                    <headline>
                        <TCEforms>
                            <label>LLL:EXT:ns_theme_hohenlohe/Resources/Private/Language/locallang_flex.xlf:general.headline</label>                            
                            <config>
                                <type>input</type>
                                <size>50</size>
                                <eval>trim</eval>
                            </config>
                        </TCEforms>
                    </headline>
                    <companies>
                        <title>LLL:EXT:ns_theme_hohenlohe/Resources/Private/Language/locallang_flex.xlf:general.companies</title>
                        <section>1</section>
                        <type>array</type>
                        <el>
                            <container>
                                <type>array</type>
                                <title>LLL:EXT:ns_theme_hohenlohe/Resources/Private/Language/locallang_flex.xlf:general.addCompanies</title>
                                <el>
                                    <logo>
                                        <TCEforms>
                                            <label>LLL:EXT:ns_theme_hohenlohe/Resources/Private/Language/locallang_flex.xlf:general.logo</label>
                                            <config>
                                                <type>group</type>
                                                <internal_type>db</internal_type>
                                                <appearance>
                                                    <elementBrowserType>file</elementBrowserType>
                                                    <elementBrowserAllowed>gif,jpg,jpeg,png,svg</elementBrowserAllowed>
                                                </appearance>
                                                <allowed>sys_file</allowed>
                                                <size>1</size>
                                                <minitems>1</minitems>
                                                <maxitems>1</maxitems>
                                                <show_thumbs>1</show_thumbs>
                                            </config>
                                        </TCEforms>
                                    </logo>
                                    <hoverLogo>
                                        <TCEforms>
                                            <label>LLL:EXT:ns_theme_hohenlohe/Resources/Private/Language/locallang_flex.xlf:general.hoverLogo</label>
                                            <config>
                                                <type>group</type>
                                                <internal_type>db</internal_type>
                                                <appearance>
                                                    <elementBrowserType>file</elementBrowserType>
                                                    <elementBrowserAllowed>gif,jpg,jpeg,png,svg</elementBrowserAllowed>
                                                </appearance>
                                                <allowed>sys_file</allowed>
                                                <size>1</size>
                                                <minitems>1</minitems>
                                                <maxitems>1</maxitems>
                                                <show_thumbs>1</show_thumbs>
                                            </config>
                                        </TCEforms>
                                    </hoverLogo>
                                    <name>
                                        <TCEforms>
                                            <label>LLL:EXT:ns_theme_hohenlohe/Resources/Private/Language/locallang_flex.xlf:general.name</label>
                                            <config>
                                                <type>input</type>
                                                <size>50</size>
                                                <eval>trim, required</eval>
                                            </config>
                                        </TCEforms>
                                    </name>
                                    <subline>
                                        <TCEforms>
                                            <label>LLL:EXT:ns_theme_hohenlohe/Resources/Private/Language/locallang_flex.xlf:general.subline</label>
                                            <config>
                                                <type>input</type>
                                                <size>50</size>
                                                <eval>trim, required</eval>
                                            </config>
                                        </TCEforms>
                                    </subline>
                                    <modelBannerImage>
                                        <TCEforms>
                                            <label>LLL:EXT:ns_theme_hohenlohe/Resources/Private/Language/locallang_flex.xlf:general.modelBannerImage</label>
                                            <config>
                                                <type>group</type>
                                                <internal_type>db</internal_type>
                                                <appearance>
                                                    <elementBrowserType>file</elementBrowserType>
                                                    <elementBrowserAllowed>gif,jpg,jpeg,png,svg</elementBrowserAllowed>
                                                </appearance>
                                                <allowed>sys_file</allowed>
                                                <size>1</size>
                                                <minitems>1</minitems>
                                                <maxitems>1</maxitems>
                                                <show_thumbs>1</show_thumbs>
                                            </config>
                                        </TCEforms>
                                    </modelBannerImage>
                                    <modelBannerTitle>
                                        <TCEforms>
                                            <label>LLL:EXT:ns_theme_hohenlohe/Resources/Private/Language/locallang_flex.xlf:general.modelBannerTitle</label>
                                            <config>
                                                <type>input</type>
                                                <size>50</size>
                                                <eval>trim, required</eval>
                                            </config>
                                        </TCEforms>
                                    </modelBannerTitle>
                                    <modelContent>
                                        <TCEforms>
                                            <label>LLL:EXT:ns_theme_hohenlohe/Resources/Private/Language/locallang_flex.xlf:general.modelContent</label>
                                            <config>
                                                <type>text</type>
                                                <rows>5</rows>
                                                <cols>30</cols>
                                                <eval>trim, required</eval>
                                                <enableRichtext>1</enableRichtext>
                                            </config>
                                        </TCEforms>
                                    </modelContent>
                                </el>
                            </container>
                        </el>
                    </companies>
                </el>
            </ROOT>
        </sDEF>
    </sheets>
</T3DataStructure>

<T3DataStructure>
    <sheets>
        <sDEF>
            <ROOT>
                <TCEforms>
                    <sheetTitle>Fields</sheetTitle>
                </TCEforms>
                <type>array</type>
                <el>
                    <overline>
                        <TCEforms>
                            <label>LLL:EXT:ns_theme_hohenlohe/Resources/Private/Language/locallang_flex.xlf:general.overline</label>
                            <config>
                                <type>input</type>
                                <size>50</size>
                                <eval>trim</eval>
                            </config>
                        </TCEforms>
                    </overline>
                    <headline>
                        <TCEforms>
                            <label>LLL:EXT:ns_theme_hohenlohe/Resources/Private/Language/locallang_flex.xlf:general.headline</label>                            
                            <config>
                                <type>input</type>
                                <size>50</size>
                                <eval>trim</eval>
                            </config>
                        </TCEforms>
                    </headline>
                    <accordions>
                        <title>LLL:EXT:ns_theme_hohenlohe/Resources/Private/Language/locallang_flex.xlf:general.addmore</title>
                        <section>1</section>
                        <type>array</type>
                        <el>
                            <container>
                                <type>array</type>
                                <title>LLL:EXT:ns_theme_hohenlohe/Resources/Private/Language/locallang_flex.xlf:accordion.title</title>
                                <el>
                                    <image>
                                        <TCEforms>
                                            <label>LLL:EXT:ns_theme_hohenlohe/Resources/Private/Language/locallang_flex.xlf:general.image</label>
                                            <config>
                                                <type>group</type>
                                                <internal_type>db</internal_type>
                                                <appearance>
                                                    <elementBrowserType>file</elementBrowserType>
                                                    <elementBrowserAllowed>jpg,jpeg,png,svg</elementBrowserAllowed>
                                                </appearance>
                                                <allowed>sys_file</allowed>
                                                <size>1</size>
                                                <minitems>1</minitems>
                                                <maxitems>1</maxitems>
                                                <show_thumbs>1</show_thumbs>
                                            </config>
                                        </TCEforms>
                                    </image>
                                    <headline>
                                        <TCEforms>
                                            <label>LLL:EXT:ns_theme_hohenlohe/Resources/Private/Language/locallang_flex.xlf:general.headline</label>
                                            <config>
                                                <type>input</type>
                                                <size>50</size>
                                                <eval>trim, required</eval>
                                            </config>
                                        </TCEforms>
                                    </headline>
                                    <content>
                                        <TCEforms>
                                            <label>LLL:EXT:ns_theme_hohenlohe/Resources/Private/Language/locallang_flex.xlf:general.content</label>
                                            <config>
                                                <type>text</type>
                                                <rows>5</rows>
                                                <cols>30</cols>
                                                <eval>trim, required</eval>
                                                <enableRichtext>1</enableRichtext>
                                            </config>
                                        </TCEforms>
                                    </content>
                                    <refContentElement>
                                        <TCEforms>
                                            <label>LLL:EXT:ns_theme_hohenlohe/Resources/Private/Language/locallang_flex.xlf:general.refContentElement</label>
                                            <config>
                                                <type>group</type>
                                                <internal_type>db</internal_type>
                                                <allowed>tt_content</allowed>
                                            </config>
                                        </TCEforms>
                                    </refContentElement>
                                    <records>
                                        <TCEforms>
                                            <label>LLL:EXT:ns_theme_hohenlohe/Resources/Private/Language/locallang_flex.xlf:general.elementReference</label>
                                            <config>
                                                <type>group</type>
                                                <internal_type>db</internal_type>
                                                <allowed>tx_fafcompanies_domain_model_company</allowed>
                                            </config>
                                        </TCEforms>
                                    </records>
                                </el>
                            </container>
                        </el>
                    </accordions>
                </el>
            </ROOT>
        </sDEF>
    </sheets>
</T3DataStructure>
